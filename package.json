{
  "name": "resume",
  "version": "1.0.0",
  "description": "My résumé, written in Pug, then rendered to HTML and PDF.",
  "main": "index.js",
  "scripts": {
    "html": "pug --pretty brian-rutledge-resume.pug",
    "pdf": "src/html2pdf.js brian-rutledge-resume.html",
    "build": "npm run html && npm run pdf",
    "watch": "onchange --initial '**/*.pug' '**/*.js' -- npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bhrutledge/resume.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bhrutledge/resume/issues"
  },
  "homepage": "https://github.com/bhrutledge/resume#readme",
  "devDependencies": {
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "onchange": "^4.1.0",
    "pug-cli": "^1.0.0-alpha6"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "puppeteer": "^1.7.0"
  }
}
